{
  "name": "backend",
  "version": "1.0.0",
  "repository": "https://github.com/HOGENT-Web/webservices-karine-2122-PieBomm.git",
  "author": "Pieter Bommele <pieter.bommele@student.hogent.be>",
  "license": "MIT",
  "source": "server.js",
  "scripts": {
    "worker": "node index.js",
    "start": "node src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'"
  },
  "prisma": {
    "seed": "ts-node ./src/prisma/seed.ts",
    "schema": "./src/prisma/schema.prisma"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "@prisma/client": "^3.5.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.2.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.10.7",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "jest": "^27.3.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "koa": "^2.13.3",
    "koa-bodyparser": "^4.3.0",
    "koa-joi-router": "^8.0.0",
    "koa-jwt": "^4.0.3",
    "koa-passport": "^4.0.1",
    "koa-session": "^6.2.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.13",
    "mysql2": "^2.3.0",
    "nanoid": "^3.1.30",
    "next": "^12.0.4",
    "nodemailer": "^6.7.0",
    "passport": "^0.5.0",
    "passport-headerapikey": "^1.2.2",
    "pino": "^7.4.1",
    "pino-pretty": "^7.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "supertest": "^6.1.6",
    "winston": "^3.3.3",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/config": "^0.0.40",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.177",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^16.11.12",
    "@types/pino": "^7.0.5",
    "nodemon": "^2.0.15",
    "parcel": "^2.0.1",
    "prisma": "^3.5.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.3",
    "winston": "^3.3.3"
  }
}

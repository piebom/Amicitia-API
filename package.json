  "scripts": {
    "start": "npx env-cmd nodemon"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "@prisma/client": "^3.2.1",
    "bcrypt": "^5.0.1",
    "bookshelf": "^1.2.0",
    "crypto": "^1.0.1",
    "knex": "^0.95.11",
    "koa": "^2.13.3",
    "koa-bodyparser": "^4.3.0",
    "koa-joi-router": "^8.0.0",
    "mysql2": "^2.3.0",
    "passport": "^0.5.0",
    "passport-headerapikey": "^1.2.2",
    "prisma": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.13",
    "winston": "^3.3.3"
  }
}
